*{
	padding: 0;
	margin: 0;
	box-sizing: border-box;
}
body{

}

.container{
	min-height: 1px;
	padding: 0 15px;
	width: 100%;
	margin: 0 auto;
	max-width: 1290px;
	&:before, &:after{
		content: "";
		display: table;
	}
	&:after{
		clear: both;
	}
}
.row{
	margin: 0 -15px;
	&:before, &:after{
		content: "";
		display: table;
	}
	&:after{
		clear: both;
	}
}

.site-header{
	padding: 0;
}




.header-top{
		background: #f2f2f2;
		padding: 8px 0;
    	line-height: 1.4;
	    /*display: -webkit-box;
	    display: -ms-flexbox;
	    display: flex;
	    -webkit-box-align: center;
	    -ms-flex-align: center;
            align-items: center;
	    -webkit-box-pack: justify;
	    -ms-flex-pack: justify;
            justify-content: space-between;
    	-ms-flex-wrap: wrap;
        flex-wrap: wrap;*/
	.grid__item{
		display: block;
		padding: 0 15px;
		@media(min-width: 992px){
			float: left;
			min-height: 1px;
			width: 75%;
		}
		.site-header__icons-wrapper{
			/*-webkit-box-pack: end;
			-ms-flex-pack: end;
			justify-content: flex-end;*/
		}
	}
}

.grid_item_number{
	padding: 0 15px;
		@media(min-width: 992px){
			float: left;
			min-height: 1px;
			width: 25%;			
		}
	a{ 
		font-family: Lato;
		font-weight: 600;
		font-size: 12px;
		color: #393939;
	}
				
}

.header-bottom{
		position: relative;
		padding: 30px 0;
	    display: -webkit-box;
	    display: -ms-flexbox;
	    display: flex;
	    -webkit-box-align: center;
	    -ms-flex-align: center;
	    align-items: center;
	    -webkit-box-pack: start;
	    -ms-flex-pack: start;
	    justify-content: flex-start;
	    position: relative;
	    min-height: 100px;
    .row{
    	display: -webkit-box;
	    display: -ms-flexbox;
	    display: flex;
	    -webkit-box-align: center;
	    -ms-flex-align: center;
	    align-items: center;
	    -webkit-box-pack: start;
	    -ms-flex-pack: start;
	    justify-content: flex-start;
	    .header-logo-wrap{
	  		@media(min-width: 992px){
		    	position: absolute;
			    left: 50%;
			    top: 50%;
			    -webkit-transform: translate(-50%, -50%);
			    -ms-transform: translate(-50%, -50%);
		        transform: translate(-50%, -50%);
			}
	    	h1{
	    		margin: 0;
	    	}
	    } 
    }
    &:after{
    	content: "";
    	position: absolute;
    	left: 50%;
    	top: 100%;
    	z-index: 1;
		-webkit-transform: translate(-50%, 0);
		-ms-transform: translate(-50%, 0);
		transform: translate(-50%, 0);
    	


    	width: 0; 
		height: 0; 
		border-left: 180px solid transparent;
		border-right: 180px solid transparent;
		border-top: 60px solid #fff;
    }
}

.header-logo-wrap{
	  		@media(min-width: 992px){
		    	position: absolute;
			    left: 50%;
			    top: 50%;
			    -webkit-transform: translate(-50%, -50%);
			    -ms-transform: translate(-50%, -50%);
		        transform: translate(-50%, -50%);
			}
	    	h1{
	    		margin: 0;
	    	}
	    }

a.site-header__logo-link{
	font-family: "KrinkesPersonalUse", sans-serif;
    text-transform: capitalize;
    font-size: 72px;
    color: #000;
    font-weight: 400;
}

.header-main-menu{ 
	display: none;
	@media (min-width: 992px){
		display: block;
	}
	ul{
		li{
			a{
				font-family: 'Playfair Display', serif;
    			font-style: italic;
    			color: #000;
    			font-size: 18px;   
				span{
					font-family: 'Playfair Display', serif;
    				font-style: italic;
	    			color: #000;
	    			font-size: 18px;
				}
			}
		}
	}
}

.site-header__icons-wrapper{
	> ul{
		/*display: -webkit-box;
	    display: -ms-flexbox;
	    display: flex;
	    -webkit-box-align: center;
	    -ms-flex-align: center;
        align-items: center;*/
        font-size: 0;
		li{
			position: relative;
			padding: 0 20px;
			font-size: 14px;
			text-transform: uppercase;
			display: inline-block;
			&:after{
				content: " | ";
				position: absolute;
				padding: 0;
				right: 0;
				top: 50%;
				-webkit-transform: translate(0, -50%);		
				-ms-transform: translate(0, -50%);	
				transform: translate(0, -50%);
			}
			&:last-child{
				&:after{
					display: none;
				}
			}
		}
	}
}






.slideshow__text-content-list{
	.html-text{
		width: 100%;
	    max-width: 280px;
	    margin: 0 auto;
	    border: 2px solid #fff;
	    padding: 10px;
	    border-radius: 2px;
		h1{
			border: 1px solid #fff;
    		padding: 20px;
    		margin: 0;
			span{
				font-family: Lato, sans-serif;
				font-weight: 700;
				display: block;

				&.c_top{
					color: #fff;
					font-size: 26px;
				}
				&.c_title{
					color: #000;
					font-size: 70px;
				}
				&.c_middle{
					position: relative;
					color: #fff;
					font-size: 26px;
					span{
						position: relative;
						&:first-child{
							&:before,
							&:after{
								content: "";
								width: 20px;
								background: #fff;
								height: 1px;   
							}
							&:after{
								width: 15px;
							}
						}
						&:last-child{
							&:before,
							&:after{
								content: "";
								width: 20px;
								background: #fff;
								height: 1px;
							}
							&:after{
								width: 15px;
							}
						}
					}
				}
				&.c_bottom{
					color: #000;
					font-size: 40px;
				}
			}
		}
	}
}



.custom-info{
		.custom-blocks-3{
			padding: 0 15px;
			display: flex;
			align-items: stretch;
			li{
				position: relative;
				padding: 30px;
				height: 100%;
				background: #d9bf8f;
				color: #fff; 
				width: 33.333%;
				&:nth-child(2){
					background: #000;
				}
			}
		}
	}

	

.custom_three_blocks{
	.custom_three_blocks_inner{
		ul{
			display: flex;
			align-items: stretch;
			li{
				@media(min-width: 768px){
					width: 33.333%;
				}
				background-color: #d9bf8f;
				color: #fff;
				&:nth-child(2){
					background-color: #000;
				}
				.inner_img{}
				.inner_text{
					h3{
						font-size: 30px;
						font-family: Lato, sans-serif;
						font-weight: 700;
						text-transform: uppercase;
					}
					p{
						font-size: 14px;
						font-family: Lato, sans-serif;
						font-weight: 400;
					}
				}
			}
		}
	}  
}

i.fas, i.far, i.fad{
	font-family:'FontAwesome';
	font-style: normal;
}

i.fa-life-ring,
i.fa-clock,
i.fa-plane{
	font-size: 36px;
}
i.fa-clock{
	font-family:'FontAwesome';
	&:before{
		content: "\f017";
	}
}


