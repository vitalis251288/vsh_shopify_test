*{
	padding: 0;
	margin: 0;
	box-sizing: border-box;
}
body{

}

.container{
	min-height: 1px;
	padding: 0 15px;
	width: 100%;
	margin: 0 auto;
	max-width: 1290px;
	&:before, &:after{
		content: "";
		display: table;
	}
	&:after{
		clear: both;
	}
}
.row{
	margin: 0 -15px;
	&:before, &:after{
		content: "";
		display: table;
	}
	&:after{
		clear: both;
	}
}

.site-header{
	padding: 0;
}




.header-top{
		background: #f2f2f2;
		padding: 8px 0;
    	line-height: 1.4;
	    /*display: -webkit-box;
	    display: -ms-flexbox;
	    display: flex;
	    -webkit-box-align: center;
	    -ms-flex-align: center;
            align-items: center;
	    -webkit-box-pack: justify;
	    -ms-flex-pack: justify;
            justify-content: space-between;
    	-ms-flex-wrap: wrap;
        flex-wrap: wrap;*/
	.grid__item{
		display: block;
		padding: 0 15px;
		@media(min-width: 992px){
			float: left;
			min-height: 1px;
			width: 75%;
		}
		.site-header__icons-wrapper{
			/*-webkit-box-pack: end;
			-ms-flex-pack: end;
			justify-content: flex-end;*/
		}
	}
}

.grid_item_number{
	padding: 0 15px;
		@media(min-width: 992px){
			float: left;
			min-height: 1px;
			width: 25%;			
		}
	a{ 
		font-family: Lato;
		font-weight: 600;
		font-size: 12px;
		color: #393939;
	}
				
}

.header-bottom{
		position: relative;
		padding: 30px 0;
	    display: -webkit-box;
	    display: -ms-flexbox;
	    display: flex;
	    -webkit-box-align: center;
	    -ms-flex-align: center;
	    align-items: center;
	    -webkit-box-pack: start;
	    -ms-flex-pack: start;
	    justify-content: flex-start;
	    position: relative;
	    min-height: 100px;
    .row{
    	display: -webkit-box;
	    display: -ms-flexbox;
	    display: flex;
	    -webkit-box-align: center;
	    -ms-flex-align: center;
	    align-items: center;
	    -webkit-box-pack: start;
	    -ms-flex-pack: start;
	    justify-content: flex-start;
	    .header-logo-wrap{
	  		@media(min-width: 992px){
		    	position: absolute;
			    left: 50%;
			    top: 50%;
			    -webkit-transform: translate(-50%, -50%);
			    -ms-transform: translate(-50%, -50%);
		        transform: translate(-50%, -50%);
			}
	    	h1{
	    		margin: 0;
	    	}
	    } 
    }
    &:after{
    	content: "";
    	position: absolute;
    	left: 50%;
    	top: 100%;
    	z-index: 1;
		-webkit-transform: translate(-50%, 0);
		-ms-transform: translate(-50%, 0);
		transform: translate(-50%, 0);
    	


    	width: 0; 
		height: 0; 
		border-left: 180px solid transparent;
		border-right: 180px solid transparent;
		border-top: 60px solid #fff;
    }
}

.header-logo-wrap{
	  		@media(min-width: 992px){
		    	position: absolute;
			    left: 50%;
			    top: 50%;
			    -webkit-transform: translate(-50%, -50%);
			    -ms-transform: translate(-50%, -50%);
		        transform: translate(-50%, -50%);
			}
	    	h1{
	    		margin: 0;
	    	}
	    }

a.site-header__logo-link{
	font-family: "KrinkesPersonalUse", sans-serif;
    text-transform: capitalize;
    font-size: 72px;
    color: #000;
    font-weight: 400;
}

.header-main-menu{ 
	display: none;
	@media (min-width: 992px){
		display: block;
	}
	ul{
		li{
			a{
				font-family: 'Playfair Display', serif;
    			font-style: italic;
    			color: #000;
    			font-size: 18px;   
				span{
					font-family: 'Playfair Display', serif;
    				font-style: italic;
	    			color: #000;
	    			font-size: 18px;
				}
			}
		}
	}
}

.site-header__icons-wrapper{
	> ul{
		/*display: -webkit-box;
	    display: -ms-flexbox;
	    display: flex;
	    -webkit-box-align: center;
	    -ms-flex-align: center;
        align-items: center;*/
        font-size: 0;
		li{
			position: relative;
			padding: 0 20px;
			font-size: 14px;
			text-transform: uppercase;
			display: inline-block;
			&:after{
				content: " | ";
				position: absolute;
				padding: 0;
				right: 0;
				top: 50%;
				-webkit-transform: translate(0, -50%);		
				-ms-transform: translate(0, -50%);	
				transform: translate(0, -50%);
			}
			&:last-child{
				&:after{
					display: none;
				}
			}
		}
	}
}





.slideshow__overlay::before{
	display: none;
}
.slideshow__text-content-list{
	.html-text{
		width: 100%;
	    max-width: 280px;
	    margin: 0 auto;
	    border: 2px solid #fff;
	    padding: 10px;
	    border-radius: 2px;
		h1{
			border: 1px solid #fff;
    		padding: 20px;
    		margin: 0;
			span{
				font-family: Lato, sans-serif;
				font-weight: 700;
				display: block;
				text-transform: uppercase;

				&.c_top{
					color: #fff;
					font-size: 26px;
				}
				&.c_title{
					color: #000;
					font-size: 70px;
				}
				&.c_middle{
					position: relative;
					display: flex;
					align-items: baseline;
					justify-content: space-between;
					color: #fff;
					font-size: 26px;
					text-align: center;
					.c_middle_title{
						display: inline-block;
						position: relative;
						z-index: 2;
						width: 40%;
						position: static;
						height: auto;
						background: transparent;
						&:before{
							display: none;
						}
					}
					span{
						position: relative;
						background: #fff;
						width: 30%;
						height: 1px;
						&:before{
							content: "";
							width: 35px;
							background: #fff;
							height: 1px;   
							display: inline-block;
						}
						&:first-child{
							&:before{
								position: absolute;
    							top: -10px;
    							right: 0;
							}
						}
						&:last-child{
							&:before{
								position: absolute;
    							top: -10px;
    							left: 0;
							}
						}
					}
				}
				&.c_bottom{
					color: #000;
					font-size: 40px;
				}
			}
		}
	}
}



.custom-info{
		.custom-blocks-3{
			padding: 0 15px;
			display: flex;
			align-items: stretch;
			li{
				position: relative;
				padding: 30px;
				height: 100%;
				background: #d9bf8f;
				color: #fff; 
				width: 33.333%;
				&:nth-child(2){
					background: #000;
				}
			}
		}
	}

	

.custom_three_blocks{
	.custom_three_blocks_inner{
		ul{
			@media(min-width: 768px){
				display: -webkit-box;
				display: -ms-flexbox;
				display: flex;
				-webkit-box-align: stretch;
				-ms-flex-align: stretch;
				align-items: stretch;
			}
			li{
				position: relative;
				padding: 0;
				line-height: 1;
				z-index: 2;
				color: #fff;
				@media(min-width: 768px){
					width: 33.333%;
					margin: 0 15px;
				}
				.inner_img{
					padding: 0 30px 0 0;
				}
				.inner_text{
					h3{
						font-size: 30px;
						font-family: Lato, sans-serif;
						font-weight: 700;
						text-transform: uppercase;
						margin: 0;
						color: #fff;
					}
					p{
						font-size: 14px;
						font-family: Lato, sans-serif;
						font-weight: 400;
						color: #fff;
					}
				}
				> div{
					width: 100%;
					display: -webkit-box;
					display: -ms-flexbox;
					display: flex;
					-webkit-box-align: center;
					-ms-flex-align: center;
					align-items: center;
					padding: 25px 35px;
					background-color: #d9bf8f;
					&:before,
					&:after{
						display: none;
						@media(min-width: 768px){						
							content: "";
							position: absolute;
							width: 0;
							height: 0;
							display: block;
						}
					}
				}
				&:nth-child(1){
				> div{
					&:after{
						right: -30px;
					    top: 0;
					    width: 0;
					    height: 0;
					    border-top: 50px solid transparent;
					    border-left: 30px solid #d9bf8f;
					    border-bottom: 50px solid transparent;
						}
					}
				}
				&:nth-child(2){
					> div{
						z-index: 1;
						background-color: #000;
						&:before{
							left: 0;
						    border-top: 50px solid transparent;
						    border-left: 30px solid #fff;
						    border-bottom: 50px solid transparent;
						}
						&:after{
							right: 0;
							border-top: 50px solid transparent;
							border-right: 30px solid #fff;
							border-bottom: 50px solid transparent;
						}
					}
				}
				&:nth-child(3){
					> div{
						&:after{
							left: -30px;
							border-top: 50px solid transparent;
							border-right: 30px solid #d9bf8f;
							border-bottom: 50px solid transparent;
						}
					}
				}
			}
		}
	}  
}

